[user]
	name = Kiichiro Kyle NAKA
	email = knaka@ayutaya.com
[core]
	quotepath = false
	excludesfile = /Users/knaka/.gitignore_global
	autoCRLF = false
	pager = LESSCHARSET=utf-8 less
	symlinks = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = current
[alias]
	df = diff --ignore-space-change
	dflast = "!f() { git  diff --ignore-space-change $1~1 $1; };f"
	st = status
	cm = commit
	co = checkout
	cotr = checkout --track
	gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	smfetch = fetch --recurse-submodules
	unstage = reset HEAD
	push-current = !git push origin $(git rev-parse --abbrev-ref HEAD) "$@"
	spush = stash push -u -k
	spop = stash pop
[rebase]
	autosquash = true
[ghq]
	root = /Users/knaka/repos
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
