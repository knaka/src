{
  "extends": "../tsconfig.paths.json",
  "compilerOptions": {
    // "baseUrl": ".",
    // "paths": {
    //   "@/*": ["./*"]
    // },
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "next.config.js",
    ".next/types/**/*.ts",
    "build/next/types/**/*.ts",
    "build/next/types/**/*.ts",
    "build/next-dev/types/**/*.ts"
  ],
  "exclude": [
    "worker",
    "functions",
    "node_modules"
  ]
}
